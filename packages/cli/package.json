    "lint": "eslint src/**/*.ts",
    "test": "bun test"
  },
  "dependencies": {
    "@devx/stack": "workspace:*",
    "@devx/recipes": "workspace:*",
    "@oclif/core": "^3.0.0",
    "commander": "^11.1.0",
    "prompts": "^2.4.2",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@oclif/plugin-legacy": "^2.0.22"
  },
  "oclif": {
    "bin": "devx",
    "dirname": "devx",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-legacy"
    ],
    "topicSeparator": " "
  }
