{
  "name": "@devx/cli",
  "version": "0.1.0",
  "description": "DevX CLI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "devx": "./dist/index.js"
  },
  "files": [
    "dist",
    "oclif.manifest.json",
    "README.md"
  ],
  "scripts": {
